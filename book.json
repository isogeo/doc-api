{
    "plugins": [
        "ga",
        "html-minifier",
        "image-captions",
        "page-toc",
        "sitemap-general",
        "timeline",
        "toggle-headers",
        "swagger"
    ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-22345848-2"
        },
        "html-minifier": {
            "decodeEntities": true,
            "html5": true,
            "removeEmptyElements": false
        },
        "image-captions": {
            "variable_name": "pictures",
            "caption": "Fig. _BOOK_IMAGE_NUMBER_ : _CAPTION_",
            "images": {
                "1.1.1": {
                    "skip": true
                }
            }
        },
        "sitemap-general": {
            "prefix": "http://help.isogeo.com/api/"
        },
        "theme-api": {
            "split": true,
            "languages": [
                {
                    "lang": "py",
                    "name": "Python",
                    "default": true
                },
                {
                    "lang": "js",
                    "name": "JavaScript"
                },
                {
                    "lang": "cs",
                    "name": "C# .NET"
                }
            ]
        },
        "page-toc": {
            "position": "top",
            "selector": ".markdown-section h2, .markdown-section h3, .markdown-section h4"
        }
    },
    "variables": {
        "author": "Isogeo",
        "pictures": []
    }
}