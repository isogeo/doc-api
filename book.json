{
    "plugins": ["page-toc",
                "timeline",
                "swagger"
                ],
    "pluginsConfig": {
        "theme-api": {
            "split": true,
            "languages": [
                {
                    "lang": "py",
                    "name": "Python",
                    "default": true
                },
                {
                    "lang": "js",
                    "name": "JavaScript"
                },
                {
                    "lang": "cs",
                    "name": "C# .NET"
                }
            ]
        },
      "page-toc": {
                  "position": "top",
                  "selector": ".markdown-section h2, .markdown-section h3, .markdown-section h4"
                  }

    }
}
